<Page
    x:Class="Platform.WinPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Platform"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Width="1280"
    Height="720">

    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="Assets/Jungle.png" Stretch="UniformToFill" />
        </Grid.Background>

        <Image Source="Assets/happyhahmo.png" Margin="-24,286,678,56"/>
        <Image Source="Assets/youwin.png" Margin="191,32,205,490" />

        <Button x:Name="BackButton"
        Click="BackButton_Click" Width="159" Height="78" BorderBrush="{x:Null}" Margin="559,286,0,356">
            <Button.Background>
                <ImageBrush ImageSource="Assets\WoodenBack.png"/>
            </Button.Background>
        </Button>

    </Grid>
</Page>
